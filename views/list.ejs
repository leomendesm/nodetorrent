<%- include('common/navbar') %>
   	<div class="row">
   	<center>
   	<div class="col l8 offset-l2 s12">
   	<div class="collection">
			<% files.forEach((file)=>{
				var link;
				let separated = file.split('\/');
				let count = separated.length;
				if(separated[count-2] === "files"){
					link = separated[count-1];
				}else{
					link = separated[count-2] +"\/" + separated[count-1];
				}
			%>
		<span class="collection-item"><a href="/files/<%= link %>"  download><%= separated[count-1] %>  </a><a href="/files/delete/<%= link %>">Deletar</a></span>
			<% }) %>
		</div>
		</div>
			</center>
	</div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
  	<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.0/js/materialize.min.js"></script>

<%- include('common/bottom') %>
